<configuration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://sumo.dlr.de/xsd/sumoConfiguration.xsd">
    <input>
        <net-file value="grid.net.xml"/> <route-files value="traffic.rou.xml"/>
        <additional-files value="detectores.add.xml"/>
    </input>
    <time> <begin value="0"/> <end value="3600"/> </time>
    <processing> <ignore-route-errors value="true"/> </processing>
    <report> <verbose value="true"/> <duration-log.statistics value="true"/> <no-step-log value="true"/> </report>
    <gui_only> <start value="true"/> </gui_only>
</configuration>